{
  "name": "indusmind-dashboard-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js --exec \"node --enable-source-maps --import tsconfig-paths/register.js --import tsx src/server.ts\"",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "format": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "js-logging": "^0.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.10.0",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "eslint": "9.18.0",
    "nodemon": "3.1.7",
    "prettier": "3.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "4.7.1",
    "typescript": "5.7.3",
    "vitest": "4.0.17"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
